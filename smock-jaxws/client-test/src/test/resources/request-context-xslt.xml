<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
        <xsl:param name="a"/>
        <xsl:param name="b"/>
        <xsl:template match="/">
                        <plusRequest xmlns="http://javacrumbs.net/calc">
